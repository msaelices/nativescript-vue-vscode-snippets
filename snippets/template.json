{
  // base snippets
  "{N}-vue base snippet with ts and sass": {
    "prefix": "nsbase_ts_sass",
    "body": [
      "<template>",
      "   <Page>",
      "   </Page>",
      "</template>",
      " ",
      "<script lang=\"ts\">",
      "export default {",
      " ",
      "}",
      "</script>",
      " ",
      "<style lang=\"sass\" scoped>",
      " ",
      "</style>"
    ]
  },
  "{N}-vue base snippet": {
    "prefix": "nsbase",
    "body": [
      "<template>",
      "   <Page>",
      "   </Page>",
      "</template>",
      " ",
      "<script>",
      "export default {",
      " ",
      "}",
      "</script>",
      " ",
      "<style scoped>",
      " ",
      "</style>"
    ]
  },
  // Action Bar
  "{N}-vue Action Bar": {
    "prefix": "nsactionbar",
    "body": [
      "<ActionBar title=\"${1:Title}\" icon=\"\">",
      "  <NavigationButton text=\"Back\" android.systemIcon=\"ic_menu_back\" @tap=\"${2:goBack}\" />",
      "  <ActionItem icon=\"\" text=\"Left\" ios.position=\"left\" @tap=\"\" />",
      "  <ActionItem icon=\"\" text=\"Right\" ios.position=\"right\" @tap=\"\" />",
      "</ActionBar>"
    ]
  },

  // Layouts
  "{N}-vue Stack Layout": {
    "prefix": "nsstack",
    "body": [
      "<StackLayout>",
      "  $0",
      "</StackLayout>"
    ]
  },
  "{N}-vue Stack Layout Horizontal": {
    "prefix": "nsstack_horizontal",
    "body": [
      "<StackLayout orientation=\"horizontal\">",
      "  $0",
      "</StackLayout>"
    ]
  },
  "{N}-vue Wrap Layout": {
    "prefix": "nswrap",
    "body": [
      "<WrapLayout orientation=\"horizontal\" itemWidth=\"$1\" itemHeight=\"$2\">",
      "  $3",
      "</WrapLayout>"
    ]
  },
  "{N}-vue Absolute Layout": {
    "prefix": "nsabsolute",
    "body": [
      "<AbsoluteLayout>",
      "  <Label text=\"This is Label 1\" top=\"\"  right=\"\" bottom=\"\" left=\"\" />",
      "  $0",
      "</AbsoluteLayout>"
    ]
  },
  "{N}-vue Dock Layout": {
    "prefix": "nsdock",
    "body": [
      "<DockLayout stretchLastChild=\"true\">",
      "  <Label dock=\"top\" text=\"I am on top!\"/>",
      "</DockLayout>"
    ]
  },
  "{N}-vue Grid Layout": {
    "prefix": "nsgrid",
    "body": [
      "<GridLayout rows=\"$1\" columns=\"$2\">",
      "  $3",
      "</GridLayout>"
    ]
  },
  "{N}-vue Grid Layout 2x2": {
    "prefix": "nsgrid_2x2",
    "body": [
      "<GridLayout rows=\"*, *\" columns=\"*, *\">",
      "  <Label text=\"0, 0\" row=\"0\" col=\"0\" />",
      "  <Label text=\"0, 1\" row=\"0\" col=\"1\" />",
      "  <Label text=\"1, 0\" row=\"1\" col=\"0\" />",
      "  <Label text=\"1, 1\" row=\"1\" col=\"1\" />",
      "</GridLayout>"
    ]
  },
  "{N}-vue Grid Layout 2x2 Automatic Width/Height": {
    "prefix": "nsgrid_2x2_auto",
    "body": [
      "<GridLayout rows=\"auto, auto\" columns=\"auto, auto\">",
      "  <Label text=\"0, 0\" row=\"0\" col=\"0\" />",
      "  <Label text=\"0, 1\" row=\"0\" col=\"1\" />",
      "  <Label text=\"1, 0\" row=\"1\" col=\"0\" />",
      "  <Label text=\"1, 1\" row=\"1\" col=\"1\" />",
      "</GridLayout>"
    ]
  },
  "{N}-vue Grid Layout 3x3 ": {
    "prefix": "nsgrid_3x3",
    "body": [
      "<GridLayout rows=\"*, *, *\" columns=\"*, *, *\">",
      "  <Label text=\"0, 0\" row=\"0\" col=\"0\" />",
      "  <Label text=\"0, 1\" row=\"0\" col=\"1\" />",
      "  <Label text=\"0, 2\" row=\"0\" col=\"2\" />",
      "  <Label text=\"1, 0\" row=\"1\" col=\"0\" />",
      "  <Label text=\"1, 1\" row=\"1\" col=\"1\" />",
      "  <Label text=\"1, 2\" row=\"1\" col=\"2\" />",
      "  <Label text=\"2, 0\" row=\"2\" col=\"0\" />",
      "  <Label text=\"2, 1\" row=\"2\" col=\"1\" />",
      "  <Label text=\"2, 2\" row=\"2\" col=\"2\" />",
      "</GridLayout>"
    ]
  },
  "{N}-vue Grid Layout 3x3 Automatic Width/Height": {
    "prefix": "nsgrid_3x3_auto",
    "body": [
      "<GridLayout rows=\"auto, auto, auto\" columns=\"auto, auto, auto\">",
      "  <Label text=\"0, 0\" row=\"0\" col=\"0\" />",
      "  <Label text=\"0, 1\" row=\"0\" col=\"1\" />",
      "  <Label text=\"0, 2\" row=\"0\" col=\"2\" />",
      "  <Label text=\"1, 0\" row=\"1\" col=\"0\" />",
      "  <Label text=\"1, 1\" row=\"1\" col=\"1\" />",
      "  <Label text=\"1, 2\" row=\"1\" col=\"2\" />",
      "  <Label text=\"2, 0\" row=\"2\" col=\"0\" />",
      "  <Label text=\"2, 1\" row=\"2\" col=\"1\" />",
      "  <Label text=\"2, 2\" row=\"2\" col=\"2\" />",
      "</GridLayout>"
    ]
  },
  "{N}-vue Grid Layout 4x3 ": {
    "prefix": "nsgrid_4x3",
    "body": [
      "<GridLayout rows=\"*, *, *, *\" columns=\"*, *, *\">",
      "  <Label text=\"0, 0\" row=\"0\" col=\"0\" />",
      "  <Label text=\"0, 1\" row=\"0\" col=\"1\" />",
      "  <Label text=\"0, 2\" row=\"0\" col=\"2\" />",
      "  <Label text=\"1, 0\" row=\"1\" col=\"0\" />",
      "  <Label text=\"1, 1\" row=\"1\" col=\"1\" />",
      "  <Label text=\"1, 2\" row=\"1\" col=\"2\" />",
      "  <Label text=\"2, 0\" row=\"2\" col=\"0\" />",
      "  <Label text=\"2, 1\" row=\"2\" col=\"1\" />",
      "  <Label text=\"2, 2\" row=\"2\" col=\"2\" />",
      "  <Label text=\"3, 0\" row=\"3\" col=\"0\" />",
      "  <Label text=\"3, 1\" row=\"3\" col=\"1\" />",
      "  <Label text=\"3, 2\" row=\"3\" col=\"2\" />",
      "</GridLayout>"
    ]
  },
  "{N}-vue Grid Layout 4x3 Automatic Width/Height": {
    "prefix": "nsgrid_4x3_auto",
    "body": [
      "<GridLayout rows=\"auto, auto, auto, auto\" columns=\"auto, auto, auto\">",
      "  <Label text=\"0, 0\" row=\"0\" col=\"0\" />",
      "  <Label text=\"0, 1\" row=\"0\" col=\"1\" />",
      "  <Label text=\"0, 2\" row=\"0\" col=\"2\" />",
      "  <Label text=\"1, 0\" row=\"1\" col=\"0\" />",
      "  <Label text=\"1, 1\" row=\"1\" col=\"1\" />",
      "  <Label text=\"1, 2\" row=\"1\" col=\"2\" />",
      "  <Label text=\"2, 0\" row=\"2\" col=\"0\" />",
      "  <Label text=\"2, 1\" row=\"2\" col=\"1\" />",
      "  <Label text=\"2, 2\" row=\"2\" col=\"2\" />",
      "  <Label text=\"3, 0\" row=\"3\" col=\"0\" />",
      "  <Label text=\"3, 1\" row=\"3\" col=\"1\" />",
      "  <Label text=\"3, 2\" row=\"3\" col=\"2\" />",
      "</GridLayout>"
    ]
  },
  "{N}-vue Grid Layout 4x4": {
    "prefix": "nsgrid_4x4",
    "body": [
      "<GridLayout rows=\"*, *, *, *\" columns=\"*, *, *, *\">",
      "  <Label text=\"0, 0\" row=\"0\" col=\"0\" />",
      "  <Label text=\"0, 1\" row=\"0\" col=\"1\" />",
      "  <Label text=\"0, 2\" row=\"0\" col=\"2\" />",
      "  <Label text=\"0, 3\" row=\"0\" col=\"3\" />",
      "  <Label text=\"1, 0\" row=\"1\" col=\"0\" />",
      "  <Label text=\"1, 1\" row=\"1\" col=\"1\" />",
      "  <Label text=\"1, 2\" row=\"1\" col=\"2\" />",
      "  <Label text=\"1, 3\" row=\"1\" col=\"3\" />",
      "  <Label text=\"2, 0\" row=\"2\" col=\"0\" />",
      "  <Label text=\"2, 1\" row=\"2\" col=\"1\" />",
      "  <Label text=\"2, 2\" row=\"2\" col=\"2\" />",
      "  <Label text=\"2, 3\" row=\"2\" col=\"3\" />",
      "  <Label text=\"3, 0\" row=\"3\" col=\"0\" />",
      "  <Label text=\"3, 1\" row=\"3\" col=\"1\" />",
      "  <Label text=\"3, 2\" row=\"3\" col=\"2\" />",
      "  <Label text=\"3, 3\" row=\"3\" col=\"3\" />",
      "</GridLayout>"
    ]
  },
  "{N}-vue Grid Layout 4x4 Automatic Width/Height": {
    "prefix": "nsgrid_4x4_auto",
    "body": [
      "<GridLayout rows=\"auto, auto, auto, auto\" columns=\"auto, auto, auto, auto\">",
      "  <Label text=\"0, 0\" row=\"0\" col=\"0\" />",
      "  <Label text=\"0, 1\" row=\"0\" col=\"1\" />",
      "  <Label text=\"0, 2\" row=\"0\" col=\"2\" />",
      "  <Label text=\"0, 3\" row=\"0\" col=\"3\" />",
      "  <Label text=\"1, 0\" row=\"1\" col=\"0\" />",
      "  <Label text=\"1, 1\" row=\"1\" col=\"1\" />",
      "  <Label text=\"1, 2\" row=\"1\" col=\"2\" />",
      "  <Label text=\"1, 3\" row=\"1\" col=\"3\" />",
      "  <Label text=\"2, 0\" row=\"2\" col=\"0\" />",
      "  <Label text=\"2, 1\" row=\"2\" col=\"1\" />",
      "  <Label text=\"2, 2\" row=\"2\" col=\"2\" />",
      "  <Label text=\"2, 3\" row=\"2\" col=\"3\" />",
      "  <Label text=\"3, 0\" row=\"3\" col=\"0\" />",
      "  <Label text=\"3, 1\" row=\"3\" col=\"1\" />",
