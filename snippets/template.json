  // Views
  "{N}-vue Scroll View": {
    "prefix": "nsscrollview",
    "body": [
      "<ScrollView>",
      "  $0",
      "</ScrollView>"
    ]
  },
  "{N}-vue List View": {
    "prefix": "nslistview",
    "body": [
      "<ListView for=\"item in ${1:items}\" @itemTap=\"onItemTap\">",
      "  <v-template>",
      "    <StackLayout>",
      "      <Label :text=\"item.text\" />$0",
      "    </StackLayout>",
      "  </v-template>",
      "</ListView>"
    ]
  },
  "{N}-vue Tab View": {
    "prefix": "nstabview",
    "body": [
      "<TabView :selectedIndex=\"${1:tabIndex}\">",
      "  <TabViewItem title=\"${2:Tab1}\">",
      "    <StackLayout>",
      "      <Label text=\"This is Label in Tab 1\" />",
      "    </StackLayout>",
      "  </TabViewItem>",
      "  <TabViewItem title=\"${3:Tab2}\">",
      "    <StackLayout>",
      "      <Label text=\"This is Label in Tab 2\" />",
      "    </StackLayout>",
      "  </TabViewItem>",
      "</TabView>",
      "$0"
    ]
  },
  "{N}-vue Web View": {
    "prefix": "nswebview",
    "body": [
      "<WebView :src=\"someUrl | pathToLocalFile | htmlString\" />",
      "$0"
    ]
  },

  // Widgets
  "{N}-vue Page": {
    "prefix": "nspage",
    "body": [
      "<Page>",
      "  $0",
      "</Page>"
    ]
  },
  "{N}-vue Image": {
    "prefix": "nsimg",
    "body": [
      "<Image :src=\"$1\" stretch=\"none\" />",
      "$0"
    ]
  },
  "{N}-vue Label": {
    "prefix": "nslabel",
    "body": [
      "<Label text=\"$1\" textWrap=\"true\" />",
      "$0"
    ]
  },
  "{N}-vue Button": {
    "prefix": "nsbtn",
    "body": [
      "<Button text=\"$1\" @tap=\"$2\" />",
      "$0"
    ]
  },
  "{N}-vue Text Field": {
    "prefix": "nstextfield",
    "body": [
      "<TextField hint=\"$1\" text=\"{{ $2 }}\" />",
      "$0"
    ]
  },
  "{N}-vue Password Field": {
    "prefix": "nspassword",
    "body": [
      "<TextField hint=\"Enter your password.\" text=\"${1:Password}\" secure=\"true\" />",
      "$0"
    ]
  },
  "{N}-vue Editable TextView": {
    "prefix": "nstextview_editable",
    "body": [
      "<TextView hint=\"$1\" text=\"{{ $2 }}\" editable=\"true\" />",
      "$0"
    ]
  },
  "{N}-vue Non-Editable TextView": {
    "prefix": "nstextview_non-editable",
    "body": [
      "<TextView text=\"$1\" editable=\"false\" />",
      "$0"
    ]
  },
  "{N}-vue SearchBar": {
    "prefix": "nssearchbar",
    "body": [
      "<SearchBar id=\"${1:searchBar}\" hint=\"${2:Search}\" text=\"$3\" clear=\"${4:onClear}\" submit=\"${5:onSubmit}\" />",
      "$6"
    ]
  },
  "{N}-vue Switch": {
    "prefix": "nsswitch",
    "body": [
      "<Switch checked=\"$1\" />",
      "$0"
    ]
  },
  "{N}-vue Progress Bar": {
    "prefix": "nsprogress",
    "body": [
      "<Progress value=\"$1\" />",
      "$0"
    ]
  },
  "{N}-vue Slider": {
    "prefix": "nsslider",
    "body": [
      "<Slider minValue=\"$1\" maxValue=\"$2\" value=\"{{ $3 }}\" />",
      "$0"
    ]
  },
  "{N}-vue Date Picker": {
    "prefix": "nsdate",
    "body": [
      "<DatePicker day=\"$1\" month=\"$2\" year=\"$3\"></DatePicker>",
      "$0"
    ]
  },
  "{N}-vue Time Picker": {
    "prefix": "nstime",
    "body": [
      "<TimePicker hour=\"$1\" minute=\"$2\"></TimePicker>",
      "$0"
    ]
  },
  "{N}-vue List Picker": {
    "prefix": "nslistpicker",
    "body": [
      "<ListPicker items=\"$1\" selectedIndex=\"$2\"></ListPicker>",
      "$0"
    ]
  },
  "{N}-vue Activity Indicator": {
    "prefix": "nsloading",
    "body": [
      "<ActivityIndicator width=\"100\" height=\"100\" busy=\"$1\" />",
      "$0"
    ]
  },
  "{N}-vue Segmented Bar": {
    "prefix": "nssegmentedbar",
    "body": [
      "<SegmentedBar selectedIndex=\"{{ selectedIndex }}\" @selectedIndexChange=\"$1\">",
      "  <SegmentedBarItem title=\"First\" />",
      "  <SegmentedBarItem title=\"Second\" />",
      "  <SegmentedBarItem title=\"Third\" />",
      "  $0",
      "</SegmentedBar>"
    ]
  },
  "{N}-vue Border": {
    "prefix": "nsborder",
    "body": [
      "<Border borderRadius=\"$1\" borderWidth=\"$2\" borderColor=\"#000000\">",
      "  $0",
      "</Border>"
    ]
  },

  // Attributes
  "{N}-vue Icon Attribute": {
    "prefix": "icon",
    "body": "icon=\"res://$1\"$0"
  },
  	"{N}-vue Text Wrap Attribute": {
    "prefix": "wrap",
    "body": "textWrap=\"true\"$0"
  },
  "{N}-vue Horizontal Alignment Attribute": {
    "prefix": "halign",
    "body": "horizontalAlignment=\"$1\"$0"
  },
  "{N}-vue Vertical Alignment Attribute": {
    "prefix": "valign",
    "body": "verticalAlignment=\"$1\"$0"
  },
  "{N}-vue Visibility Attribute": {
    "prefix": "visibility",
    "body": ":visibility=\"isItemVisible ? 'visible' : 'collapsed'\"$0"
  },
  "{N}-vue Stretch Attribute": {
    "prefix": "stretch",
    "body": "stretch=\"fill$1\"$0"
  },
  "{N}-vue Keyboard Type Attribute": {
    "prefix": "kbtype",
    "body": "keyboardType=\"email$1\"$0"
  }
}
